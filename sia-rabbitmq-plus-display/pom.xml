            <id>pro</id>
            <properties>
                <spring.cloud.config.profile>pro</spring.cloud.config.profile>
                <spring.cloud.config.label>master</spring.cloud.config.label>
                <spring.cloud.config.uri>http://10.120.64.57:17005</spring.cloud.config.uri>
            </properties>
