            <properties>
                <spring.cloud.config.profile>test</spring.cloud.config.profile>
                <spring.cloud.config.label>master</spring.cloud.config.label>
                <spring.cloud.config.uri>http://10.143.128.215:17005</spring.cloud.config.uri>
            </properties>
